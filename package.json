{
    "name": "hello-agents-ts",
    "version": "0.1.0",
    "description": "灵活、可扩展的多智能体框架 - 基于 Datawhale Hello-Agents 教程，使用 TypeScript 实现",
    "keywords": [],
    "author": "SleepSheep",
    "license": "ISC",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "start": "tsx src/index.ts",
        "build": "pnpm run sync:version && tsc",
        "serve": "node dist/index.js",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write .",
        "test": "tsx src/test/simpleAgent.ts",
        "test:first": "tsx --env-file .env src/test/firstAgent.ts",
        "sync:version": "node script/syncVersion.js"
    },
    "packageManager": "pnpm@10.18.1",
    "dependencies": {
        "axios": "^1.13.2",
        "dotenv": "^17.2.3",
        "openai": "^6.8.1",
        "tavily": "^1.0.2"
    },
    "devDependencies": {
        "@types/node": "^24.10.0",
        "@typescript-eslint/eslint-plugin": "^8.46.3",
        "@typescript-eslint/parser": "^8.46.3",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3"
    }
}